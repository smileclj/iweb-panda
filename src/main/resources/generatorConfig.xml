     		<columnOverride  column="Sex" property="sex" javaType="java.lang.Integer" jdbcType="INTEGER"/>
     		<columnOverride  column="CreateTime" property="createTime" javaType="java.util.Date"/>
     		<columnOverride  column="LastLoginTime" property="lastLoginTime" javaType="java.util.Date"/>
     		<columnOverride  column="visible" property="visible" javaType="java.lang.Boolean" jdbcType="TINYINT"/>
